const nos=[12,2,3,45,67,89,12,34,2,3,4];
const over18=nos.filter(fun1);
console.log(over18);
function fun1(value){
    return value>18;
}
//filter


//reduce AND reduceRight
const no=[92,10,5,12,8];
const sub=no.reduce(fun2);
const sub1=no.reduceRight(fun3);
console.log(sub);
console.log(sub1);

function fun2(total,value,index,array){
    return total-value;
}
function fun3(total,value,index,array){
    return total-value;
}


