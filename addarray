let no=[1,2,3,4];
console.log(no.push(5,6)); 
console.log(no);//end 
console.log(no.unshift(11,12)); // beginning
console.log(no);
console.log(no.splice(1,0,'a','b')); //middle
console.log(no);
console.log(no.splice(1,3,'a','b')); //middle
console.log(no);
console.log(no.splice(2,3,'a','b'));
console.log(no);
