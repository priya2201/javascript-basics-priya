let course=[
    {id:1,name:'a'},
    {id:2,name:'b'},

];
console.log(course.includes({id:1,name:'a'}));

//find method
var found=course.find(function(course){
    return course.name === 'a';

});
console.log(found);
var found2=course.find(course => course.name==='b');
