 const arr1=[1,2,4,56,78];

 function show1(array,searchElement){
 for(let n of array){
  if(n%2==0 && n==searchElement){
    console.log("no is divisble by 2");}
   else{
         console.log("No is not there and it's not divisible by 2");
   }}
}

