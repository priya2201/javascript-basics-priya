let x=300;
let y=200;
let z=200;


console.log(x+y);
console.log(x-y);
console.log(x*y);
console.log(x/y);
console.log(x%y);
console.log(x**y);
console.log(x++);
console.log(++x);
console.log(x*=y);//x=302*200 ,x=60400
console.log(x+=y);//x=60400+200=60600
console.log(x-=y);
console.log(y%=x);
console.log(x<=y);
console.log(x>=y);
console.log(x==y);
console.log(x=y);//x=200
console.log(y===z);
console.log('smile'==='smile');
console.log(2==='2');
console.log(2=='2');
console.log(NaN==NaN);
console.log(NaN!==NaN);
