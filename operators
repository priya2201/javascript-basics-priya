//read ,write and execute permissions in bitwise operators

const rprmson=4;
const wprmson=2;
const eprmson=1;

let mprmson=0;
mprmson= mprmson | rprmson | eprmson;
console.log(mprmson);
let message=(mprmson & eprmson)? 'Yes' : "No";
console.log(message);























