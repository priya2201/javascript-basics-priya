let meena=true;
let heena=false;
console.log(meena || heena);
console.log(meena && heena);
const result=console.log(!meena);
console.log(result);

let usercolor="blue"; //if we choose undefined then the selected color turns to red not blue
let defaultcolor="red";
console.log(usercolor || defaultcolor);
console.log(usercolor && defaultcolor);
console.log(false || 'pia');
console.log(false ||'pia' || 1 || 2);
